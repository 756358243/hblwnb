	<script>
		$(document).ready(function() {

						
			//datepicker
			$('.input-group.date').datepicker({
				todayBtn : "linked",
				keyboardNavigation : false,
				forceParse : false,
				calendarWeeks : true,
				autoclose : true,
				format : 'yyyy-mm-dd 00:00:00'
			});

			$('.i-checks').iCheck({
				checkboxClass : 'icheckbox_square-green',
				radioClass : 'iradio_square-green',
			});

			$('#djxxForm').ajaxForm({

				dataType : 'json',
				success : function(ret) {
					if (ret.code == 0) {
						swal({
							title : ret.info,
							type : "success",
							timer : 2000
						});
				    } else {
				        	swal({
									title : ret.info,
									type : "error",
									timer : 2000
								});
							
				    }
				},
				error : function(data) {
					swal({
						title : "服务器错误，请稍候访问！",
						type : "error",
						timer : 2000
					});
				}
			});
			
			$('#wjxxForm').ajaxForm({

				dataType : 'json',
				success : function(ret) {
					if (ret.code == 0) {
						swal({
