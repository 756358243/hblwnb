<body>
    <div id="wrapper">
       #parse("/header.vm")

    <div id="page-wrapper" class="gray-bg" >
        #parse("/news.vm")
        
        <div class="row wrapper border-bottom white-bg">
            <div class="col-lg-10">
                <h2>车辆检验信息</h2>
                <ol class="breadcrumb crumb">
                   <li>
                        <span>当前位置:</span>
                    </li>
                    <li>
                        <a href="index.html">车辆检测信息</a>
                    </li>
